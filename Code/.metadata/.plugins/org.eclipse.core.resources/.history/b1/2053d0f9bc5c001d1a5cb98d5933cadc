#include <fsm_simple_buttons.h>
int counter = 0;
void fsm_simple_buttons_run (){
	switch(status){
	case RESET:
		//TO DO
		counter = 0;

		break;
	case INC:
		//TO DO
		if(counter > 0) counter++;
		else{
			//NOTHING
		}
		break;
	case DEC:
		//TO DO
		if(counter > 0) counter--;
		else{
			//NOTHING
		}
		break;
	default:
		break;
	}
}
